{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch ",
    "prettier-write": "prettier --write 'src/**/*.+(ts|tsx|json|js|jsx)' '*.+(ts|tsx|json|js|jsx)' '**/*.+(css|scss)'",
    "i18n:extract": "formatjs extract './**/*.ts*' --ignore ./node_modules --ignore './**/*.d.ts' --ignore './**/*.spec.*' --out-file './i18n/frontend.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format crowdin",
    "i18n:compile": "formatjs compile-folder --format crowdin ./i18n/locales ./src/translations"
  },
  "dependencies": {
    "@emotion/react": "11.10.8",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.8",
    "@emotion/cache": "11.10.8",
    "@fontsource/roboto": "4.5.8",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.12.2",
    "@next/font": "13.3.4",
    "@types/node": "18.16.3",
    "@types/react-dom": "18.2.1",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "classnames": "2.3.2",
    "eslint": "8.39.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "13.3.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-compat": "4.1.4",
    "eslint-plugin-formatjs": "4.10.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "msw": "1.2.1",
    "next": "13.3.4",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.4.2",
    "sass": "1.62.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@formatjs/cli": "6.1.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.2.0",
    "@types/testing-library__jest-dom": "5.14.5",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "whatwg-fetch": "3.6.2"
